{
  "name": "simple-chat-client",
  "version": "1.0.0",
  "description": "Let's get it",
  "main": "index.js",
  "scripts": {
    "build-server": "webpack --config lib/server.js",
    "build-browser": "webpack --config lib/browser.js",
    "compile": "babel webpack-configs -d lib",
    "clear": "rm -rf ./build/* && rm -rf ./lib/*",
    "build": "npm run clear && npm run compile && npm run build-browser && npm run build-server",
    "dev": "NODE_ENV=development npm run build && npm run start",
    "prod": "NODE_ENV=production npm run build && npm run start",
    "start": "node ./build/server.js"
